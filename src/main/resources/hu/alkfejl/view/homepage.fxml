                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="50.0">
                    <columns>
                        <TableColumn fx:id="searchNameColumn1" maxWidth="425.0" minWidth="425.0" prefWidth="425.0"
                                     text="Név"/>
                        <TableColumn fx:id="searchCategoryColumn1" maxWidth="425.0" minWidth="425.0"
                                     prefWidth="425.0" text="Kategória"/>
                        <TableColumn fx:id="searchRuleTableColumn1" maxWidth="425.0" minWidth="425.0"
                                     prefWidth="425.0" text="Szabályzat"/>
                    </columns>
                </TableView>
                <TextField fx:id="searchRoomInterestField" layoutX="470.0" layoutY="14.0" prefHeight="25.0"
