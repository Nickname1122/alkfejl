<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane maxHeight="720.0" maxWidth="1280.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0"
            prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hu.alkfejl.view.controller.HomePage">
    <Label fx:id="title" layoutX="594.0" layoutY="10.0" text="Chat App">
        <font>
            <Font size="23.0"/>
        </font>
    </Label>
    <TabPane fx:id="mainTabPane" layoutX="1.0" layoutY="50.0" maxHeight="670.0" maxWidth="1280.0" minHeight="670.0"
             minWidth="1280.0" prefHeight="670.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE">
        <Tab fx:id="rooms" text="Szobák">
            <AnchorPane fx:id="roomsAnchor" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TableView fx:id="roomsTableView" layoutX="200.0" layoutY="60.0" maxHeight="285.0"
                           maxWidth="600.0" minHeight="285.0" minWidth="600.0" prefHeight="285.0"
                           prefWidth="600.0" AnchorPane.bottomAnchor="37.0" AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="roomNameColumn" maxWidth="320.0" minWidth="320.0" prefWidth="320.0"
                                     text="Név"/>
                        <TableColumn fx:id="roomCategoryColumn" maxWidth="320.0" minWidth="320.0"
                                     prefWidth="320.0" text="Kategória"/>
                        <TableColumn fx:id="roomRuleColumn" maxWidth="320.0" minWidth="320.0" prefWidth="320.0"
                                     text="Szabályzat"/>
                        <TableColumn fx:id="enterToRoom" maxWidth="320.0" minWidth="320.0" prefWidth="320.0"
                                     text="Belépés"/>
                    </columns>
                </TableView>
            </AnchorPane>
        </Tab>
        <Tab fx:id="search" text="Szobakereső">
            <AnchorPane fx:id="searchAnchor" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TableView fx:id="searchTableView" layoutX="1.0" layoutY="50.0" maxHeight="670.0"
                           maxWidth="1280.0" minHeight="670.0" minWidth="1280.0" prefHeight="670.0"
                           prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
                    <columns>
                        <TableColumn fx:id="searchNameColumn" maxWidth="425.0" minWidth="425.0"
                                     prefWidth="425.0" text="Név"/>
                        <TableColumn fx:id="searchCategoryColumn" maxWidth="425.0" minWidth="425.0"
                                     prefWidth="425.0" text="Kategória"/>
                        <TableColumn fx:id="searchRuleTableColumn" maxWidth="425.0" minWidth="425.0"
                                     prefWidth="425.0" text="Szabályzat"/>
                    </columns>
                </TableView>
                <TextField layoutX="470.0" layoutY="14.0" prefHeight="25.0" prefWidth="170.0"
                           promptText="Szoba kereső"/>
                <Button fx:id="searchRoomButton" layoutX="654.0" layoutY="14.0" mnemonicParsing="false"
                        onAction="#searchRoom" text="Keresés"/>
            </AnchorPane>
        </Tab>
        <Tab text="Felhasználó kereső">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TextField layoutX="491.0" layoutY="8.0" promptText="Felhasználó kereső"/>
                <TableView layoutY="39.0" maxHeight="670.0" maxWidth="1280.0" minHeight="670.0" minWidth="1280.0"
                           prefHeight="670.0" prefWidth="1280.0">
                    <columns>
                        <TableColumn maxWidth="640.0" minWidth="640.0" prefWidth="640.0" text="Felhasználónév"/>
                        <TableColumn maxWidth="640.0" minWidth="640.0" prefWidth="640.0" text="Érdeklődési kör"/>
                    </columns>
                </TableView>
                <Button fx:id="searchUserButton" layoutX="653.0" layoutY="7.0" mnemonicParsing="false"
                        onAction="#searchUser" text="Keresés"/>
            </AnchorPane>
        </Tab>
        <Tab text="Profil">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <Label alignment="CENTER" layoutX="371.0" layoutY="124.0" prefHeight="82.0" prefWidth="153.0"
                       text="Felhasználónév">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" layoutX="389.0" layoutY="363.0" prefHeight="83.0" prefWidth="116.0"
                       text="Életkor">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" layoutX="376.0" layoutY="247.0" prefHeight="69.0" prefWidth="142.0"
                       text="Érdeklődési kör">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="profilUsernameLabel" alignment="CENTER" layoutX="562.0" layoutY="119.0"
                       maxHeight="92.0" maxWidth="300.0" minHeight="92.0" minWidth="300.0" prefHeight="92.0"
                       prefWidth="300.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="profilInterestLabel" alignment="CENTER" layoutX="562.0" layoutY="236.0"
                       maxHeight="92.0" maxWidth="300.0" minHeight="92.0" minWidth="300.0" prefHeight="92.0"
                       prefWidth="300.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="profilAgeLabel" alignment="CENTER" layoutX="562.0" layoutY="358.0" maxHeight="92.0"
                       maxWidth="300.0" minHeight="92.0" minWidth="300.0" prefHeight="92.0" prefWidth="300.0">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
            </AnchorPane>
        </Tab>
    </TabPane>
    <Button fx:id="refreshButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#refreshTable"
            text="Refresh"/>
</AnchorPane>
